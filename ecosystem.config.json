{
  "apps": [
    {
      "name": "rollup-service",
      "cwd": "./rollup",
      "interpreter": "~/.bun/bin/bun",
      "script": "bun",
      "args": "start",
      "watch": true,
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "name": "app-service",
      "interpreter": "~/.bun/bin/bun",
      "script": "bun",
      "args": "dev",
      "watch": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
